{
  "name":           "ikagaka.ghost.js",
  "version":        "0.3.2",
  "description":    "Ikagaka Shiori Wrapper",
  "url":            "https://github.com/ikagaka/Ghost.js",
  "keywords":       ["nar", "ikagaka", "unstable", "ukagaka"],
  "scripts": {
    "copy": "npm run enc; npm run aya5; npm run kawari; npm run kawari7; npm run satori; npm run yaya; npm run nativeshiori;",
    "enc":    "cp ./node_modules/encoding-japanese/encoding.min.js ./vendor",
    "aya5":   "cp ./node_modules/aya5.js/aya5_240/aya5.js ./vendor",
    "kawari": "cp ./node_modules/kawari.js/kawari-828/build/mach/em/kawari.js ./vendor",
    "kawari7":"cp ./node_modules/kawari7.js/kawari-750-kdt/src/shiori_js/kawari7.js ./vendor",
    "satori": "cp ./node_modules/libsatori.js/satoriya/satori/libsatori.js ./vendor",
    "yaya":   "cp ./node_modules/yaya.js/aya5/yaya.js ./vendor",
    "nativeshiori": "cp ./node_modules/nativeshiori/nativeshiori.js ./vendor",
    "watch": "coffee -cw ./*.coffee",
    "build": "coffee -c ./*.coffee"
  },
  "repository": {
    "type":         "git",
    "url":          "https://github.com/ikagaka/Ghost.js"
  },
  "dependencies": {
    "ikagaka.nar.js": "",
    "encoding-japanese": "",
    "nativeshiori": ">= 0.0.1",
    "yaya.js": "",
    "libsatori.js": "",
    "aya5.js": "",
    "kawari.js": "",
    "kawari7.js": "",
    "libsatori.js": ""
  },
  "devDependencies": {
    "coffee-script": ""
  },
  "main":           "./Ghost.js",
  "author":         "Ikagaka",
  "license":        "MIT",
  "contributors":   ["narazaka", "legokichi"]
}
